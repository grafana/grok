            "type": "string"
          },
          "connectedDashboards": {
            "type": "integer",
            "format": "int64"
          },
          "created": {
            "type": "string",
            "format": "date-time"
          },
          "updated": {
            "type": "string",
            "format": "date-time"
          },
          "createdBy": {
            "$ref": "#/components/schemas/LibraryElementDTOMetaUser"
          },
          "updatedBy": {
            "$ref": "#/components/schemas/LibraryElementDTOMetaUser"
          }
        },
        "$schema": "http://json-schema.org/draft-04/schema#"
      },
      "LibraryElementDTOMetaUser": {
        "type": "object",
