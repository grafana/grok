        "$schema": "http://json-schema.org/draft-04/schema#"
      },
      "LegendDisplayMode": {
        "description": "TODO docs\nNote: \"hidden\" needs to remain as an option for plugins compatibility",
        "type": "string",
        "enum": [
          "list",
          "table",
          "hidden"
        ],
        "$schema": "http://json-schema.org/draft-04/schema#"
      },
